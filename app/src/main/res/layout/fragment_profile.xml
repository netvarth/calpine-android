<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/layout_toolbar"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:background="@color/hometopbar"
            android:orientation="vertical">

            <include
                android:id="@+id/toolbar"
                layout="@layout/custom_toolbar" />
        </LinearLayout>
        <!-- TODO: Update blank fragment layout -->

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/default_padding">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:id="@+id/lprofile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/action_background"
                    android:elevation="15dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_gravity="center"
                        android:layout_margin="15.0dip"
                        android:scaleType="fitXY"
                        android:src="@drawable/user_icon">

                    </ImageView>

                    <LinearLayout
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:orientation="vertical">

                        <com.jaldeeinc.jaldee.custom.CustomTextViewBold
                            android:id="@+id/tv_userName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:letterSpacing="0.03"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="@color/dark_black"
                            android:textSize="18dp">

                        </com.jaldeeinc.jaldee.custom.CustomTextViewBold>


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="8dp">

                            <ImageView
                                android:id="@+id/image"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/call_icon"
                                app:tint="@color/dark_blue">

                            </ImageView>

                            <com.jaldeeinc.jaldee.custom.CustomTextViewMedium
                                android:id="@+id/tv_phoneNumber"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:layout_toRightOf="@id/image"
                                android:text=""
                                android:textColor="@color/title_color"
                                android:textSize="15dp">

                            </com.jaldeeinc.jaldee.custom.CustomTextViewMedium>


                        </RelativeLayout>


                        <LinearLayout
                            android:id="@+id/ll_mailId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:visibility="gone">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:src="@drawable/emailicon_sized"
                                app:tint="@color/dark_blue">

                            </ImageView>

                            <com.jaldeeinc.jaldee.custom.CustomTextViewMedium
                                android:id="@+id/tv_mailId"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="5dp"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="sravanworkinfo@gmail.com"
                                android:textColor="@color/title_color"
                                android:textSize="15dp">

                            </com.jaldeeinc.jaldee.custom.CustomTextViewMedium>

                        </LinearLayout>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/iv_edit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="15.0dip"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_edit"
                        app:tint="@color/dark_blue">

                    </ImageView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/padding7"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/name"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"

                            android:drawableLeft="@drawable/icon_updateprofile"
                            android:drawablePadding="10dp"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/updateprofile"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding2"
                            android:src="@drawable/icon_angle_right" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/view_border"
                    android:visibility="gone" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/ljcash"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/ic_jaldee_wallet"></ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="My Wallet"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lmember"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="3dp"
                            android:src="@drawable/icon_addmember"></ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="10dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/member"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lchangepwd"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/icon_password"></ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/changepwd"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/padding5"
                    android:layout_marginRight="@dimen/padding5"
                    android:layout_marginBottom="@dimen/padding5"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/lchangeemail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/padding7"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:text="@string/changeEmail"
                            android:textColor="@color/title_grey"
                            android:textSize="@dimen/txtsize_hint" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lchangeph"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/icon_change_number"></ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/changePh"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lterm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/ynw_logo"></ImageView>


                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/aboutus"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lcontactus"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/icon_contactus"></ImageView>


                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/contactus"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lnotification"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/icon_notification"></ImageView>


                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/notifications"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lappfeed"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="1dp"
                            android:src="@drawable/icon_feedback"></ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/sendappfeed"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/lshare"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/icon_share">

                        </ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/shareynw"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_white_bg_rect"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/llogout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="8dp"
                            android:src="@drawable/icon_logout"></ImageView>

                        <com.jaldeeinc.jaldee.custom.CustomTextViewSemiBold
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="16dp"
                            android:layout_weight="1"
                            android:gravity="left"
                            android:justificationMode="inter_word"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/padding3"
                            android:text="@string/logout"
                            android:textColor="@color/black"
                            android:textSize="@dimen/txtsize_hint"
                            android:textStyle="bold" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="0.1"
                            android:paddingRight="@dimen/padding5"
                            android:src="@drawable/icon_angle_right"
                            app:tint="@color/gray" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="@dimen/padding45"
                    android:layout_marginEnd="25dp"
                    android:background="@color/view_border" />
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</FrameLayout>


